<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f6f6f6"
    tools:context=".ChatActivity">

    <TableLayout
        android:layout_width="411dp"
        android:layout_height="59dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="544dp"
        tools:ignore="MissingConstraints">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="51dp"
                android:layout_height="match_parent"
                android:padding="5dp"
                app:srcCompat="@drawable/_72_1726160_add_picture_switch_camera_icon_white_png" />

            <EditText
                android:id="@+id/edit_text"
                android:layout_width="306dp"
                android:layout_height="60dp"
                android:layout_marginBottom="16dp"
                android:background="#ffffff"
                android:hint="@string/hint" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="61dp"
                android:layout_height="63dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="16dp"
                android:padding="8dp"
                app:srcCompat="@drawable/send_white_192x192" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="411dp"
        android:layout_height="59dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="1dp"
        tools:ignore="MissingConstraints">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:id="@+id/textView9"
                android:layout_width="40dp"
                android:layout_height="57dp"
                android:layout_weight="30"
                android:padding="15dp"
                android:text="@string/Nameview11"
                android:textAlignment="viewStart"
                android:textColor="@color/cardview_light_background"
                android:textSize="24sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

        </TableRow>

    </TableLayout>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="48dp"
        android:layout_height="53dp"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/avatar_3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="307dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="48dp"
        android:layout_height="53dp"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/avatar_3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="138dp" />

    <ImageView
        android:id="@+id/activity_main"
        android:layout_width="279dp"
        android:layout_height="106dp"
        android:layout_marginStart="70dp"
        android:background="#D5CDCD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.171" />

    <ImageView
        android:id="@+id/activity_second"
        android:layout_width="285dp"
        android:layout_height="48dp"
        android:layout_marginStart="124dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.418" />

    <ImageView
        android:id="@+id/activity_third"
        android:layout_width="220dp"
        android:layout_height="43dp"
        android:layout_marginStart="70dp"
        android:background="#D5CDCD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.567" />


</androidx.constraintlayout.widget.ConstraintLayout>
